{
  "version": 2,
  "projects": {
    "a1": {
      "root": "apps/a1",
      "projectType": "application",
      "sourceRoot": "apps/a1",
      "targets": {
        "pre-commit": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "tfenv use",
              "terraform init -backend=false",
              "terraform validate",
              "terraform fmt -recursive",
              "tflint",
              "tfsec",
              "terraform-docs markdown table --output-file README.md --output-mode inject ."
            ],
            "cwd": "apps/a1",
            "parallel": false
          }
        },
        "plan": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "tfenv use",
              "terraform init",
              "terraform plan --out=a1.tfplan"
            ],
            "cwd": "apps/a1",
            "parallel": false
          }
        }
      },
      "tags": null
    },
