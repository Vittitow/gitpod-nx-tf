{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "terraform-lib",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "tags": {
      "type": "string",
      "description": "Add tags to the library (separated by commas)"
    }
  },
  "required": ["name"]
}
